  },
  "devDependencies": {
    "@jackfranklin/rollup-plugin-markdown": "^0.4.0",
    "@mapbox/eslint-config-mapbox": "^3.0.0",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
